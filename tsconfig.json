{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "target": "ESNext",
        "allowJs": false,
        "esModuleInterop": true,
        "strict": true,
        "paths": {
            "@prototype-extensions": ["./years/prototype-extensions"],
            "@common": ["./years/common"],
        }
    },
    "exclude": ["node_modules"],
    "typeAcquisition": {
        "enable": true,
    }
}
